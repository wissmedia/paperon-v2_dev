<!DOCTYPE html>
<html lang="en">
<!-- HEAD PARTIALS -->
<%- include('../partials/head.ejs') %>

  <body>
    <!-- NAV PARTIALS -->
    <%- include('../partials/nav.ejs') %>
      <div class="container">
        <div class="card-container">
          <% if (Menus) { %>
            <% Menus.forEach(menu=> { %>
              <a href="/info<%= menu.link %> ">
                <div class="card-block fadeInKiri">
                  <h2>
                    <i class="<%= menu.icon %> "></i>
                    <%= menu.label %>
                  </h2>
                </div>
              </a>
              <% }) %>
                <% } else { %>
                  <div class="card-block fadeInKiri">
                    <h2>
                      <i class="fas fa-times-circle"></i>
                      Tidak ada data
                    </h2>
                  </div>
                  <% } %>
        </div>
      </div>

      <!-- MODAL AUTH -->
      <%- include('../partials/modal-auth.ejs') %>
        <!-- FOOTER PARTIALS -->
        <%- include('../partials/footer.ejs') %>
          <!-- SCRIPT PARTIALS -->
          <%- include('../partials/script.ejs') %>

  </body>

</html>